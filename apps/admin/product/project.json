{
  "name": "admin-product",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/admin/product/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/admin/product",
        "main": "apps/admin/product/src/main.ts",
        "tsConfig": "apps/admin/product/tsconfig.app.json",
        "webpackConfig": "apps/admin/product/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "admin-product:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "admin-product:build:development"
        },
        "production": {
          "buildTarget": "admin-product:build:production"
        }
      }    },
    "test-cloudinary": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node apps/admin/product/src/cloudinary-test.js"
      }
    },
    "serve:nodemon": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dotenv -e apps/admin/product/.env.development -- nodemon --watch apps/admin/product/src --ext ts,js,json --exec ts-node apps/admin/product/src/main.ts"
      }
    }
  },
  "tags": []
}
